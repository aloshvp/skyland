{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@common/*": [
        "app/components/common/*"
      ],
      "@home/*": [
        "app/components/home/*"
      ],
      "@styles/*": [
        "public/styles/*"
      ],
      "@public/*": [
        "public/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@functions/*": ["app/components/functions/*"],
      "@components/*": [
        "app/components/*"
      ],
       "@lib/*": ["lib/*"],
    }
  }
}